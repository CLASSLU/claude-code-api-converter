eApiKey('deepl');\n   497→\n   498→    if (!apiKey) {\n   499→      throw new Error('未设置DeepL API密钥，请先在设置中配置');\n   500→    }\n   501→\n   502→    const url = 'https://api-free.deepl.com/v2/translate';\n   503→\n   504→    const controller = new AbortController();\n   505→    const timeoutId = setTimeout(() => controller.abort(), 10000);\n   506→\n   507→    try {\n   508→      const response = await fetch(url, {\n   509→        method: 'POST',\n   510→        headers: {\n   511→          'Authorization': `DeepL-Auth-Key ${apiKey}`,\n   512→          'Content-Type': 'application/json'\n   513→        },\n   514→        body: JSON.stringify({\n   515→          text: [text],\n   516→          target_lang: targetLang.toUpperCase().replace('-', '')\n   517→        }),\n   518→        signal: controller.signal\n   519→      });\n   520→\n   521→      if (!response.ok) {\n   522→        throw new Error(`HTTP错误: ${response.status}`);\n   523→      }\n   524→\n   525→      // 安全地解析JSON响应\n   526→      let data;\n   527→      try {\n   528→        const responseText = await response.text();\n   529→        if (!responseText || !responseText.trim()) {\n   530→          throw new Error('API返回空响应');\n   531→        }\n   532→        data = JSON.parse(responseText);\n   533→      } catch (parseError) {\n   534→        console.error('DeepL翻译API JSON解析失败:', parseError);\n   535→        throw new Error(`DeepL翻译API响应格式错误: ${parseError.message}`);\n   536→      }\n   537→\n   538→      if (data && data.translations && data.translations[0]) {\n   539→        return data.translations[0].text;\n   540→      }\n   541→\n   542→      throw new Error('DeepL翻译API响应格式错误');\n   543→    } finally {\n   544→      clearTimeout(timeoutId);\n   545→    }\n   546→  }\n   547→\n   548→  // 缓存管理\n   549→  generateCacheKey(text, targetLang, service) {\n   550→    const normalizedText = text.trim().toLowerCase();\n   551→    return `${service}:${targetLang}:${this.hashText(normalizedText)}`;\n   552→  }\n   553→\n   554→  hashText(text) {\n   555→    let hash = 0;\n   556→    for (let i = 0; i < text.length; i++) {\n   557→      const char = text.charCodeAt(i);\n   558→      hash = ((hash << 5) - hash) + char;\n   559→      hash = hash & hash;\n   560→    }\n   561→    return hash.toString(36);\n   562→  }\n   563→\n   564→  getCachedTranslation(cacheKey) {\n   565→    const entry = this.requestCache.get(cacheKey);\n   566→    if (!entry) return null;\n   567→\n   568→    if (Date.now() > entry.expiresAt) {\n   569→      this.requestCache.delete(cacheKey);\n   570→      return null;\n   571→    }\n   572→\n   573→    return entry.translation;\n   574→  }\n   575→\n   576→  cacheTranslation(cacheKey, translation) {\n   577→    if (this.requestCache.size >= this.maxCacheSize) {\n   578→      this.evictOldestCache();\n   579→    }\n   580→\n   581→    this.requestCache.set(cacheKey, {\n   582→      translation: translation,\n   583→      timestamp: Date.now(),\n   584→      expiresAt: Date.now() + 3600000 // 1小时过期\n   585→    });\n   586→  }\n   587→\n   588→  cacheError(cacheKey, error) {\n   589→    this.errorCache.set(cacheKey, {\n   590→      error: error.message,\n   591→      timestamp: Date.now(),\n   592→      expiresAt: Date.now() + 300000 // 5分钟过期\n   593→    });\n   594→  }\n   595→\n   596→  evictOldestCache() {\n   597→    let oldestKey = null;\n   598→    let oldestTime = Date.now();\n   599→\n   600→    for (const [key, entry] of this.requestCache.entries()) {\n   601→      if (entry.timestamp < oldestTime) {\n   602→        oldestTime = entry.timestamp;\n   603→        oldestKey = key;\n   604→      }\n   605→    }\n   606→\n   607→    if (oldestKey) {\n   608→      this.requestCache.delete(oldestKey);\n   609→    }\n   610→  }\n   611→\n   612→  cleanupExpiredCache() {\n   613→    const now = Date.now();\n   614→    let cleaned = 0;\n   615→\n   616→    for (const [key, entry] of this.requestCache.entries()) {\n   617→      if (now > entry.expiresAt