nt Script - 支持无字幕检测和评论区翻译\n     2→console.log(\'🎯 YouTube翻译插件 - 增强版开始加载...\');\n     3→\n     4→(function() {\n     5→    \'use strict\';\n     6→\n     7→    class YouTubeTranslator {\n     8→        constructor() {\n     9→            this.isInitialized = false;\n    10→            this.settings = {\n    11→                translationEnabled: true,\n    12→                targetLanguage: \'zh-CN\',\n    13→                translationService: \'openai\',\n    14→                showOriginal: true,\n    15→                enableNoSubtitleDetection: true,\n    16→                enableCommentTranslation: true,\n    17→                fontSize: 16,\n    18→                position: \'bottom\'\n    19→            };\n    20→\n    21→            this.lastDetectedSubtitle = \'\';\n    22→            this.lastTranslatedComment = \'\';\n    23→            this.observer = null;\n    24→            this.commentObserver = null;\n    25→            this.noSubtitleTimer = null;\n    26→\n    27→            this.init();\n    28→        }\n    29→\n    30→        async init() {\n    31→            console.log(\'🚀 开始初始化增强翻译功能...\');\n    32→\n    33→            try {\n    34→                // 确保DOM已加载\n    35→                if (document.readyState === \'loading\') {\n    36→                    await new Promise(resolve => {\n    37→                        document.addEventListener(\'DOMContentLoaded\', resolve);\n    38→                    });\n    39→                }\n    40→\n    41→                // 等待一小段时间确保DOM完全准备就绪\n    42→                await new Promise(resolve => setTimeout(resolve, 100));\n    43→\n    44→                // 加载设置\n    45→                await this.loadSettings();\n    46→\n    47→                // 创建UI元素\n    48→                this.createTranslationUI();\n    49→\n    50→                // 初始化字幕检测\n    51→                this.initSubtitleDetection();\n    52→\n    53→                // 初始化评论区翻译\n    54→                this.initCommentTranslation();\n    55→\n    56→                // 初始化无字幕检测\n    57→                this.initNoSubtitleDetection();\n    58→\n    59→                // 监听消息\n    60→                this.setupMessageListener();\n    61→\n    62→                this.isInitialized = true;\n    63→                console.log(\'✅ 增强翻译功能初始化完成\');\n    64→\n    65→                // 显示状态\n    66→                this.showStatus(\'插件已加载\', \'success\');\n    67→\n    68→            } catch (error) {\n    69→                console.error(\'初始化失败:\', error);\n    70→                this.showStatus(\'初始化失败: \' + error.message, \'error\');\n    71→            }\n    72→        }\n    73→\n    74→        async loadSettings() {\n    75→            return new Promise((resolve) => {\n    76→                if (typeof chrome !== \'undefined\' && chrome.storage && chrome.storage.sync) {\n    77→                    chrome.storage.sync.get(Object.keys(this.settings), (result) => {\n    78→                        this.settings = { ...this.settings, ...result };\n    79→                        console.log(\'📋 设置已加载:\', this.settings);\n    80→                        resolve();\n    81→                    });\n    82→                } else {\n    83→                    console.log(\'在测试环境中使用默认设置\');\n    84→                    resolve();\n    85→                }\n    86→            });\n    87→        }\n    88→\n    89→        createTranslationUI() {\n    90→            // 创建翻译容器\n    91→            const translationContainer = document.createElement(\'div\');\n    92→            translationContainer.id = \'youtube-translation-enhanced-container\';\n    93→            translationContainer.style.cssText = `\n    94→                position: fixed;\n    95→                bottom: 80px;\n    96→                left: 50%;\n    97→                transform: translateX(-50%);\n    98→                background: rgba(0, 0, 0, 0.9);\n    99→                color: white;\n   100→                padding: 12px 20px;\n   101→                border-radius: 8px;\n   102→                font-size: ${this.settings.fontSize}px;\n   103→                font-family: -apple-system, BlinkMacSystemFont, \'Segoe UI\', Arial, sans-serif;\n   104→                z-index: 10000;\n   105→                box-shadow: 0 4px 16px rgba(0,0,0,0.4);\n   106→                opacity: 0;\n   107→                transition: opacity 0.3s ease;\n   108→                pointer-events: none;\n   109→                max-width: 80%;\n   110→                text-align: center;\n   111→                border-left: 4px solid #1976d2;\n   112→            `;\n   113→            document.body.appendChild(translationContainer);\n   114→\n   115→            // 创建评论区翻译容器\n   116→            const commentTranslationContainer = document.createElement(\'div\');\n   117→            commentTranslationContainer.id = \'youtube-comment-translation-container\';\n   118→            commentTranslationContainer.style.cssText = `\n   119→                position: fixed;\n   120→                top: 50%;\n   121→                right: 20px;\n   122→                transform: translateY(-50%);\n   123→                background: rgba(33, 33, 33, 0.95);\n   124→                color: white;\n   125→                padding: 15px;\n   126→                border-radius: 8px;\n   127→                font-size: 14px;\n   128→                font-family: -apple-system, BlinkMacSystemFont, \'Segoe UI\', Arial, sans-serif;\n   129→                z-index: 9999;\n   130→                box-shadow: 0 4px 20px rgba(0,0,0,0.5);\n   131→                opacity: 0;\n   132→                transition: opacity 0.3s ease;\n   133→                max-width: 300px;\n   134→                max-height: 400px;\n   135→                overflow-y: auto;\n   136→                border-left: 3px solid #4caf50;\n   137→                display: none;\n   138→            `;\n   139→            document.body.appendChild(commentTranslationContainer);\n   140→\n   141→            // 创建无字幕提示容器\n   142→            const noSubtitleContainer = document.createElement(\'div\');\n   143→            noSubtitleContainer.id = \'youtube-no-subtitle-container\';\n   144→            noSubtitleContainer.style.cssText = `\n   145→                position: fixed;\n   146→                top: 20%;\n   147→                left: 50%;\n   148→                transform: translateX(-50%);\n   149→                background: rgba(255, 152, 0, 0.9);\n   150→                color: white;\n   151→                padding: 15px 25px;\n   152→                border-radius: 8px;\n   153→                font-size: 16px;\n   154→                font-weight: bold;\n   155→                z-index: 10001;\n   156→                box-shadow: 0 4px 20px rgba(0,0,0,0.4);\n   157→                opacity: 0;\n   158→                transition: opacity 0.3s ease;\n   159→                cursor: pointer;\n   160→                border-left: 4px solid #ff9800;\n   161→            `;\n   162→            noSubtitleContainer.innerHTML = \'🔍 未检测到字幕<br><span style="font-size: 12px; font-weight: normal; opacity: 0.8;">点击查看视频描述翻译</span>\';\n   163→            document.body.appendChild(noSubtitleContainer);\n   164→\n   165→            // 添加控制面板\n   166→            this.createControlPanel();\n   167→\n   168→            console.log(\'✅ UI元素已创建\');\n   169→        }\n   170→\n   171→        createControlPanel() {\n   172→            const controlPanel = document.createElement(\'div\');\n   173→            controlPanel.id = \'youtube-translator-control-panel\';\n   174→            controlPanel.style.cssText = `\n   175→                position: fixed;\n   176→                top: 10px;\n   177→                left: 10px;\n   178→                background: rgba(0, 0, 0, 0.8);\n   179→                color: white;\n   180→                padding: 10px;\n   181→                border-radius: 6px;\n   182→                font-size: 12px;\n   183→                z-index: 999999;\n   184→                display: flex;\n   185→                gap: 8px;\n   186→                flex-direction: column;\n   187→                min-width: 200px;\n   188→                border: 1px solid rgba(255,255,255,0.2);\n   189→            `;\n   190→\n   191→            controlPanel.innerHTML = `\n   192→                <div style="font-weight: bold; margin-bottom: 5px;">🌐 翻译控制台</div>\n   193→                <div style="display: flex; gap: 5px; align-items: center;">\n   194→                    <span>字幕翻译:</span>\n   195→                    <input type="checkbox" id="subtitle-toggle" ${this.settings.translationEnabled ? \'checked\' : \'\'} style="width: auto;">\n   196→                </div>\n   197→                <div style="display: flex; gap: 5px; align-items: center;">\n   198→                    <span>评论区翻译:</span>\n   199→                    <input type="checkbox" id="comment-toggle" ${this.settings.enableCommentTranslation ? \'checked\' : \'\'} style="width: auto;">\n   200→                </div>\n   201→                <div style="display: flex; gap: 5px; align-items: center;">\n   202→                    <span>无字幕检测:</span>\n   203→                    <input type="checkbox" id="no-subtitle-toggle" ${this.settings.enableNoSubtitleDetection ? \'checked\' : \'\'} style="width: auto;">\n   204→                </div>\n   205→                <button id="translate-description" style="margin-top: 5px; padding: 4px 8px; font-size: 11px; background: #1976d2; color: white; border: none; border-radius: 3px; cursor: pointer;">\n   206→                    翻译视频描述\n   207→                </button>\n   208→                <div style="font-size: 10px; opacity: 0.7; margin-top: 5px;">\n   209→                    目标语言: ${this.settings.targetLanguage}\n   210→                </div>\n   211→            `;\n   212→\n   213→            document.body.appendChild(controlPanel);\n   214→\n   215→            // 绑定事件\n   216→            document.getElementById(\'subtitle-toggle\').addEventListener(\'change\', (e) => {\n   217→                this.settings.translationEnabled = e.target.checked;\n   218→                this.showStatus(`字幕翻译已${e.target.checked ? \'开启\' : \'关闭\'}`, \'info\');\n   219→            });\n   220→\n   221→            document.getElementById(\'comment-toggle\').addEventListener(\'change\', (e) => {\n   222→                this.settings.enableCommentTranslation = e.target.checked;\n   223→                this.showStatus(`评论区翻译已${e.target.checked ? \'开启\' : \'关闭\'}`, \'info\');\n   224→            });\n   225→\n   226→            document.getElementById(\'no-subtitle-toggle\').addEventListener(\'change\', (e) => {\n   227→                this.settings.enableNoSubtitleDetection = e.target.checked;\n   228→                this.showStatus(`无字幕检测已${e.target.checked ? \'开启\' : \'关闭\'}`, \'info\');\n   229→            });\n   230→\n   231→            document.getElementById(\'translate-description\').addEventListener(\'click\', () => {\n   232→                this.translateVideoDescription();\n   233→            });\n   234→        }\n   235→\n   236→        initSubtitleDetection() {\n   237→            if (!this.settings.translationEnabled) return;\n   238→\n   239→            // 监听DOM变化\n   240→            this.observer = new MutationObserver((mutations) => {\n   241→                this.checkSubtitles();\n   242→            });\n   243→\n   244→            // 观察字幕容器 - 支持多种YouTube字幕选择器\n   245→            const subtitleSelectors = [\n   246→                \'.ytp-caption-segment\',\n   247→                \'.caption-row\',\n   248→                \'.caption-window\',\n   249→                \'[data-yt-subtitle]\',\n   250→                \'#captions-container .caption-text\',\n   251→                \'.ytp-caption-window-container\'\n   252→            ];\n   253→\n   254→            let subtitleContainer = null;\n   255→            for (const selector of subtitleSelectors) {\n   256→                subtitleContainer = document.querySelector(selector);\n   257→                if (subtitleContainer) {\n   258→                    console.log(`✅ 找到字幕容器: ${selector}`);\n   259→                    break;\n   260→                }\n   261→            }\n   262→\n   263→            if (!subtitleContainer) {\n   264→                subtitleContainer = document.body;\n   265→            }\n   266→\n   267→            this.observer.observe(subtitleContainer, {\n   268→                childList: true,\n   269→                subtree: true,\n   270→                characterData: true\n   271→            });\n   272→\n   273→            // 定期检查字幕（备用方案）\n   274→            setInterval(() => {\n   275→                this.checkSubtitles();\n   276→            }, 1000);\n   277→\n   278→            console.log(\'✅ 字幕检测已初始化\');\n   279→        }\n   280→\n   281→        checkSubtitles() {\n   282→            if (!this.settings.translationEnabled) return null;\n   283→\n   284→            const subtitleSelectors = [\n   285→                \'.ytp-caption-segment\',\n   286→                \'.caption-row\',\n   287→                \'[data-yt-subtitle]\',\n   288→                \'#captions-container .caption-text\',\n   289→                \'.caption-window .caption-row\'\n   290→            ];\n   291→\n   292→            let subtitleText = null;\n   293→\n   294→            for (const selector of subtitleSelectors) {\n   295→                const elements = document.querySelectorAll(selector);\n   296→                if (elements.length > 0) {\n   297→                    const texts = Array.from(elements).map(el => el.textContent.trim()).filter(text => text);\n   298→                    if (texts.length > 0) {\n   299→                        subtitleText = texts.join(\' \');\n   300→                        break;\n   301→                    }\n   302→                }\n   303→            }\n   304→\n   305→            // 检查自定义字幕事件\n   306→            if (!subtitleText && window.customCaptionData) {\n   307→                subtitleText = window.customCaptionData;\n   308→            }\n   309→\n   310→            if (subtitleText && subtitleText !== this.lastDetectedSubtitle) {\n   311→                this.lastDetectedSubtitle = subtitleText;\n   312→                console.log(\'📺 检测到字幕:\', subtitleText);\n   313→                this.translateText(subtitleText, \'subtitle\');\n   314→                return subtitleText;\n   315→            }\n   316→\n   317→            // 如果没有字幕，启动无字幕检测\n   318→            if (!subtitleText && this.settings.enableNoSubtitleDetection) {\n   319→                this.startNoSubtitleDetection();\n   320→            }\n   321→\n   322→            return subtitleText;\n   323→        }\n   324→\n   325→        initCommentTranslation() {\n   326→            if (!this.settings.enableCommentTranslation) return;\n   327→\n   328→            // 创建评论区翻译的观察器\n   329→            this.commentObserver = new MutationObserver((mutations) => {\n   330→                mutations.forEach((mutation) => {\n   331→                    if (mutation.type === \'childList\' && mutation.addedNodes.length > 0) {\n   332→                        this.checkNewComments(mutation.addedNodes);\n   333→                    }\n   334→                });\n   335→            });\n   336→\n   337→            // 观察评论区容器\n   338→            const commentContainer = document.querySelector(\'#comments\') ||\n   339→                                  document.querySelector(\'[class*="comment"]\') ||\n   340→                                  document.querySelector(\'#below\');\n   341→\n   342→            if (commentContainer) {\n   343→                this.commentObserver.observe(commentContainer, {\n   344→                    childList: true,\n   345→                    subtree: true\n   346→                });\n   347→                console.log(\'✅ 评论区翻译已初始化\');\n   348→            } else {\n   349→                // 延迟重试\n   350→                setTimeout(() => this.initCommentTranslation(), 2000);\n   351→            }\n   352→        }\n   353→\n   354→        checkNewComments(nodes) {\n   355→            if (!this.settings.enableCommentTranslation) return;\n   356→\n   357→            nodes.forEach(node => {\n   358→                if (node.nodeType === Node.ELEMENT_NODE) {\n   359→                    // 查找评论元素\n   360→                    const commentElements = node.querySelectorAll ?\n   361→                        node.querySelectorAll(\'[id*="comment"], [class*="comment"], ytd-comment-thread-renderer\') : [];\n   362→\n   363→                    commentElements.forEach(comment => {\n   364→                        const commentText = this.extractCommentText(comment);\n   365→                        if (commentText && commentText !== this.lastTranslatedComment) {\n   366→                            this.lastTranslatedComment = commentText;\n   367→                            console.log(\'💬 检测到评论:\', commentText.substring(0, 50) + \'...\');\n   368→\n   369→                            // 添加翻译按钮\n   370→                            this.addTranslationButton(comment, commentText);\n   371→                        }\n   372→                    });\n   373→                }\n   374→            });\n   375→        }\n   376→\n   377→        extractCommentText(commentElement) {\n   378→            const textSelectors = [\n   379→                \'#content-text\',\n   380→                \'.ytd-comment-renderer #content-text\',\n   381→                \'[id*="content-text"]\',\n   382→                \'.comment-text-content\'\n   383→            ];\n   384→\n   385→            for (const selector of textSelectors) {\n   386→                const textElement = commentElement.querySelector(selector);\n   387→                if (textElement && textElement.textContent.trim()) {\n   388→                    return textElement.textContent.trim();\n   389→                }\n   390→            }\n   391→\n   392→            // 备用方案：直接获取元素文本\n   393→            const text = commentElement.textContent?.trim();\n   394→            if (text && text.length > 10 && text.length < 1000) { // 合理长度的评论\n   395→                return text;\n   396→            }\n   397→\n   398→            return null;\n   399→        }\n   400→\n   401→        addTranslationButton(commentElement, commentText) {\n   402→            // 检查是否已有翻译按钮\n   403→            if (commentElement.querySelector(\'.translation-btn\')) return;\n   404→\n   405→            const translateBtn = document.createElement(\'button\');\n   406→            translateBtn.className = \'translation-btn\';\n   407→            translateBtn.textContent = \'🌐 翻译\';\n   408→            translateBtn.style.cssText = `\n   409→                background: #1976d2;\n   410→                color: white;\n   411→                border: none;\n   412→                padding: 4px 8px;\n   413→                border-radius: 4px;\n   414→                font-size: 12px;\n   415→                cursor: pointer;\n   416→                margin-left: 8px;\n   417→                transition: background 0.2s;\n   418→            `;\n   419→\n   420→            translateBtn.addEventListener(\'click\', (e) => {\n   421→                e.stopPropagation();\n   422→                this.translateText(commentText, \'comment\', commentElement);\n   423→            });\n   424→\n   425→            translateBtn.addEventListener(\'mouseenter\', () => {\n   426→                translateBtn.style.background = \'#1565c0\';\n   427→            });\n   428→\n   429→            translateBtn.addEventListener(\'mouseleave\', () => {\n   430→                translateBtn.style.background = \'#1976d2\';\n   431→            });\n   432→\n   433→            // 添加翻译结果容器\n   434→            const translationResult = document.createElement(\'div\');\n   435→            translationResult.className = \'translation-result\';\n   436→            translationResult.style.cssText = `\n   437→                background: rgba(76, 175, 80, 0.1);\n   438→                border-left: 3px solid #4caf50;\n   439→                padding: 8px 12px;\n   440→                margin-top: 8px;\n   441→                border-radius: 4px;\n   442→                font-size: 14px;\n   443→                color: #333;\n   444→                display: none;\n   445→            `;\n   446→\n   447→            // 插入按钮和结果容器\n   448→            const insertLocation = commentElement.querySelector(\'#content-text\') ||\n   449→                                 commentElement.querySelector(\'[content-text]\') ||\n   450→                                 commentElement;\n   451→            insertLocation.appendChild(translateBtn);\n   452→            insertLocation.appendChild(translationResult);\n   453→        }\n   454→\n   455→        initNoSubtitleDetection() {\n   456→            // 无字幕检测将在checkSubtitles方法中触发\n   457→            console.log(\'✅ 无字幕检测已初始化\');\n   458→        }\n   459→\n   460→        startNoSubtitleDetection() {\n   461→            if (!this.settings.enableNoSubtitleDetection) return;\n   462→\n   463→            // 清除之前的定时器\n   464→            if (this.noSubtitleTimer) {\n   465→                clearTimeout(this.noSubtitleTimer);\n   466→            }\n   467→\n   468→            // 5秒后显示无字幕提示\n   469→            this.noSubtitleTimer = setTimeout(() => {\n   470→                this.showNoSubtitlePrompt();\n   471→            }, 5000);\n   472→        }\n   473→\n   474→        stopNoSubtitleDetection() {\n   475→            if (this.noSubtitleTimer) {\n   476→                clearTimeout(this.noSubtitleTimer);\n   477→                this.noSubtitleTimer = null;\n   478→            }\n   479→\n   480→            // 隐藏无字幕提示\n   481→            const container = document.getElementById(\'youtube-no-subtitle-container\');\n   482→            if (container) {\n   483→                container.style.opacity = \'0\';\n   484→            }\n   485→        }\n   486→\n   487→        showNoSubtitlePrompt() {\n   488→            const container = document.getElementById(\'youtube-no-subtitle-container\');\n   489→            if (container) {\n   490→                container.style.opacity = \'1\';\n   491→                container.style.display = \'block\';\n   492→\n   493→                container.addEventListener(\'click\', () => {\n   494→                    this.translateVideoDescription();\n   495→                    container.style.opacity = \'0\';\n   496→                });\n   497→\n   498→                // 10秒后自动隐藏\n   499→                setTimeout(() => {\n   500→                    container.style.opacity = \'0\';\n   501→                }, 10000);\n   502→            }\n   503→        }\n   504→\n   505→        async translateVideoDescription() {\n   506→            // 查找视频描述\n   507→            const descriptionSelectors = [\n   508→                \'#description\',\n   509→                \'.ytd-video-secondary-info-renderer #description\',\n   510→                \'[slot="description"]\',\n   511→                \'.ytd-watch-metadata #description\'\n   512→            ];\n   513→\n   514→            let descriptionText = \'\';\n   515→            for (const selector of descriptionSelectors) {\n   516→                const element = document.querySelector(selector);\n   517→                if (element) {\n   518→                    descriptionText = element.textContent.trim();\n   519→                    if (descriptionText) break;\n   520→                }\n   521→            }\n   522→\n   523→            if (descriptionText) {\n   524→                console.log(\'📝 翻译视频描述:\', descriptionText.substring(0, 100) + \'...\');\n   525→                await this.translateText(descriptionText, \'description\');\n   526→            } else {\n   527→                this.showStatus(\'未找到视频描述\', \'error\');\n   528→            }\n   529→        }\n   530→\n   531→        async translateText(text, type, contextElement = null) {\n   532→            if (!text || text.trim().length === 0) return;\n   533→\n   534→            try {\n   535→                if (typeof chrome !== \'undefined\' && chrome.runtime) {\n   536→                    // 发送翻译请求到background script\n   537→                    const response = await new Promise((resolve, reject) => {\n   538→                        chrome.runtime.sendMessage({\n   539→                            action: \'translateText\',\n   540→                            text: text,\n   541→                            targetLang: this.settings.targetLanguage,\n   542→                            service: this.settings.translationService,\n   543→                            type: type\n   544→                        }, (response) => {\n   545→                            if (chrome.runtime.lastError) {\n   546→                                reject(new Error(chrome.runtime.lastError.message));\n   547→                            } else {\n   548→                                resolve(response);\n   549→                            }\n   550→                        });\n   551→                    });\n   552→\n   553→                    if (response && response.success) {\n   554→                        const translatedText = response.translation;\n   555→                        this.displayTranslation(text, translatedText, type, contextElement);\n   556→                    } else {\n   557→                        // 在调试模式下显示模拟翻译\n   558→                        this.displayTranslation(text, `[${this.settings.targetLanguage}翻译] ${text}`, type, contextElement);\n   559→                    }\n   560→                } else {\n   561→                    // 测试环境模拟翻译\n   562→                    const mockTranslation = `[${this.settings.targetLanguage}] ${text}`;\n   563→                    this.displayTranslation(text, mockTranslation, type, contextElement);\n   564→                }\n   565→            } catch (error) {\n   566→                console.error(\'翻译失败:\', error);\n   567→                this.displayTranslation(text, `[翻译错误] ${text}`, type, contextElement);\n   568→            }\n   569→        }\n   570→\n   571→        displayTranslation(original, translated, type, contextElement) {\n   572→            console.log(`🌐 ${type}翻译完成:`, translated);\n   573→\n   574→            switch (type) {\n   575→                case \'subtitle\':\n   576→                    this.displaySubtitleTranslation(original, translated);\n   577→                    break;\n   578→                case \'comment\':\n   579→                    this.displayCommentTranslation(original, translated, contextElement);\n   580→                    break;\n   581→                case \'description\':\n   582→                    this.displayDescriptionTranslation(original, translated);\n   583→                    break;\n   584→            }\n   585→\n   586→            this.showStatus(\'翻译完成\', \'success\');\n   587→        }\n   588→\n   589→        displaySubtitleTranslation(original, translated) {\n   590→            const container = document.getElementById(\'youtube-translation-enhanced-container\');\n   591→            if (container) {\n   592→                container.innerHTML = this.settings.showOriginal ?\n   593→                    `<div style="margin-bottom: 4px; font-size: 0.9em; opacity: 0.8;">${original}</div>\n   594→                     <div style="font-weight: bold;">${translated}</div>` :\n   595→                    `<div style="font-weight: bold;">${translated}</div>`;\n   596→\n   597→                container.style.opacity = \'1\';\n   598→\n   599→                // 停止无字幕检测\n   600→                this.stopNoSubtitleDetection();\n   601→\n   602→                // 6秒后隐藏\n   603→                setTimeout(() => {\n   604→                    container.style.opacity = \'0\';\n   605→                }, 6000);\n   606→            }\n   607→        }\n   608→\n   609→        displayCommentTranslation(original, translated, commentElement) {\n   610→            // 查找或创建翻译结果容器\n   611→            let resultContainer = commentElement.querySelector(\'.translation-result\');\n   612→            if (!resultContainer) {\n   613→                resultContainer = document.createElement(\'div\');\n   614→                resultContainer.className = \'translation-result\';\n   615→                resultContainer.style.cssText = `\n   616→                    background: rgba(76, 175, 80, 0.1);\n   617→                    border-left: 3px solid #4caf50;\n   618→                    padding: 8px 12px;\n   619→                    margin-top: 8px;\n   620→                    border-radius: 4px;\n   621→                    font-size: 14px;\n   622→                    color: #333;\n   623→                `;\n   624→                commentElement.appendChild(resultContainer);\n   625→            }\n   626→\n   627→            resultContainer.innerHTML = `\n   628→                <div style="font-size: 12px; color: #666; margin-bottom: 4px;">🌐 翻译结果 (${this.settings.targetLanguage})</div>\n   629→                <div>${translated}</div>\n   630→            `;\n   631→            resultContainer.style.display = \'block\';\n   632→\n   633→            // 更新按钮文本\n   634→            const translateBtn = commentElement.querySelector(\'.translation-btn\');\n   635→            if (translateBtn) {\n   636→                translateBtn.textContent = \'✅ 已翻译\';\n   637→                translateBtn.style.background = \'#4caf50\';\n   638→            }\n   639→        }\n   640→\n   641→        displayDescriptionTranslation(original, translated) {\n   642→            // 创建描述翻译模态框\n   643→            const modal = document.createElement(\'div\');\n   644→            modal.style.cssText = `\n   645→                position: fixed;\n   646→                top: 50%;\n   647→                left: 50%;\n   648→                transform: translate(-50%, -50%);\n   649→                background: white;\n   650→                color: #333;\n   651→                padding: 20px;\n   652→                border-radius: 8px;\n   653→                box-shadow: 0 4px 20px rgba(0,0,0,0.3);\n   654→                max-width: 600px;\n   655→                max-height: 400px;\n   656→                overflow-y: auto;\n   657→                z-index: 10002;\n   658→                font-family: -apple-system, BlinkMacSystemFont, \'Segoe UI\', Arial, sans-serif;\n   659→            `;\n   660→\n   661→            modal.innerHTML = `\n   662→                <div style="display: flex; justify-content: space-between; align-items: center; margin-bottom: 15px; border-bottom: 1px solid #eee; padding-bottom: 10px;">\n   663→                    <h3 style="margin: 0; color: #1976d2;">📝 视频描述翻译</h3>\n   664→                    <button onclick="this.parentElement.parentElement.remove()" style="background: none; border: none; font-size: 20px; cursor: pointer; color: #666;">✕</button>\n   665→                </div>\n   666→                <div style="margin-bottom: 10px;">\n   667→                    <strong>原文:</strong>\n   668→                    <div style="background: #f5f5f5; padding: 10px; border-radius: 4px; margin-top: 5px; font-size: 14px; line-height: 1.4;">${original}</div>\n   669→                </div>\n   670→                <div>\n   671→                    <strong>翻译 (${this.settings.targetLanguage}):</strong>\n   672→                    <div style="background: #e3f2fd; padding: 10px; border-radius: 4px; margin-top: 5px; font-size: 14px; line-height: 1.4;">${translated}</div>\n   673→                </div>\n   674→            `;\n   675→\n   676→            // 添加背景遮罩\n   677→            const overlay = document.createElement(\'div\');\n   678→            overlay.style.cssText = `\n   679→                position: fixed;\n   680→                top: 0;\n   681→                left: 0;\n   682→                right: 0;\n   683→                bottom: 0;\n   684→                background: rgba(0,0,0,0.5);\n   685→                z-index: 10