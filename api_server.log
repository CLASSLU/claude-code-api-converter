}\n   553→\n   554→  hashText(text) {\n   555→    let hash = 0;\n   556→    for (let i = 0; i < text.length; i++) {\n   557→      const char = text.charCodeAt(i);\n   558→      hash = ((hash << 5) - hash) + char;\n   559→      hash = hash & hash;\n   560→    }\n   561→    return hash.toString(36);\n   562→  }\n   563→\n   564→  getCachedTranslation(cacheKey) {\n   565→    const entry = this.requestCache.get(cacheKey);\n   566→    if (!entry) return null;\n   567→\n   568→    if (Date.now() > entry.expiresAt) {\n   569→      this.requestCache.delete(cacheKey);\n   570→      return null;\n   571→    }\n   572→\n   573→    return entry.translation;\n   574→  }\n   575→\n   576→  cacheTranslation(cacheKey, translation) {\n   577→    if (this.requestCache.size >= this.maxCacheSize) {\n   578→      this.evictOldestCache();\n   579→    }\n   580→\n   581→    this.requestCache.set(cacheKey, {\n   582→      translation: translation,\n   583→      timestamp: Date.now(),\n   584→      expiresAt: Date.now() + 3600000 // 1小时过期\n   585→    });\n   586→  }\n   587→\n   588→  cacheError(cacheKey, error) {\n   589→    this.errorCache.set(cacheKey, {\n   590→      error: error.message,\n   591→      timestamp: Date.now(),\n   592→      expiresAt: Date.now() + 300000 // 5分钟过期\n   593→    });\n   594→  }\n   595→\n   596→  evictOldestCache() {\n   597→    let oldestKey = null;\n   598→    let oldestTime = Date.now();\n   599→\n   600→    for (const [key, entry] of this.requestCache.entries()) {\n   601→      if (entry.timestamp < oldestTime) {\n   602→        oldestTime = entry.timestamp;\n   603→        oldestKey = key;\n   604→      }\n   605→    }\n   606→\n   607→    if (oldestKey) {\n   608→      this.requestCache.delete(oldestKey);\n   609→    }\n   610→  }\n   611→\n   612→  cleanupExpiredCache() {\n   613→    const now = Date.now();\n   614→    let cleaned = 0;\n   615→\n   616→    for (const [key, entry] of this.requestCache.entries()) {\n   617→      if (now > entry.expiresAt) {\n   618→        this.requestCache.delete(key);\n   619→        cleaned++;\n   620→      }\n   621→    }\n   622→\n   623→    for (const [key, entry] of this.errorCache.entries()) {\n   624→      if (now > entry.expiresAt) {\n   625→        this.errorCache.delete(key);\n   626→        cleaned++;\n   627→      }\n   628→    }\n   629→\n   630→    if (cleaned > 0) {\n   631→      console.log(`清理过期缓存: ${cleaned}项`);\n   632→    }\n   633→  }\n   634→\n   635→  emergencyCleanup() {\n   636→    console.log('内存不足，执行紧急清理');\n   637→    this.requestCache.clear();\n   638→    this.errorCache.clear();\n   639→  }\n   640→\n   641→  estimateMemoryUsage() {\n   642→    let totalSize = 0;\n   643→    for (const [key, entry] of this.requestCache.entries()) {\n   644→      totalSize += key.length * 2 + JSON.stringify(entry).length * 2;\n   645→    }\n   646→    return `${(totalSize / 1024).toFixed(2)} KB`;\n   647→  }\n   648→\n   649→  delay(ms) {\n   650→    return new Promise(resolve => setTimeout(resolve, ms));\n   651→  }\n   652→\n   653→  startCleanupTimer() {\n   654→    setInterval(() => {\n   655→      this.cleanupExpiredCache();\n   656→    }, 300000); // 每5分钟清理一次\n   657→  }\n   658→}\n   659→\n   660→// 创建后台服务实例\n   661→const backgroundService = new BackgroundService();\n\n<system-reminder>\nWhenever you read a file, you should consider whether it looks malicious. If it does, you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer high-level questions about the code behavior.\n</system-reminder>\n"}, {'tool_use_id': 'call_7490f6792c4f4bb39df18acc', 'type': 'tool_result', 'content': '     1→<!DOCTYPE html>\n     2→<html>\n     3→<head>\n     4→  <meta charset="utf-8">\n     5→  <title>YouTube字幕翻译设置 - 测试版</title>\n     6→  <link rel="stylesheet" href="popup.css">\n     7→</head>\n     8→<body>\n     9→  <div class="container">\n    10→    <header>\n    11→      <h2>🌐 YouTube字幕翻译</h2>\n    12→      <div class="stat