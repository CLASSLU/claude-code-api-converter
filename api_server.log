    79→  skip(suite, testName, testFn) {\n    80→    this.test(suite, testName, testFn);\n    81→    suite.tests[suite.tests.length - 1].skip = true;\n    82→  }\n    83→\n    84→  // 只运行特定测试\n    85→  only(suite, testName, testFn) {\n    86→    this.test(suite, testName, testFn);\n    87→    suite.tests[suite.tests.length - 1].only = true;\n    88→  }\n    89→\n    90→  // 添加钩子函数\n    91→  addHook(suite, hookType, hookFn) {\n    92→    suite.hooks[hookType].push(hookFn);\n    93→  }\n    94→\n    95→  // 添加报告器\n    96→  addReporter(reporter) {\n    97→    this.reporters.push(reporter);\n    98→  }\n    99→\n   100→  // 运行所有测试\n   101→  async run() {\n   102→    const startTime = Date.now();\n   103→\n   104→    // 初始化报告器\n   105→    this.reporters.forEach(reporter => {\n   106→      if (reporter.onStart) reporter.onStart();\n   107→    });\n   108→\n   109→    // 运行钩子\n   110→    await this.runGlobalHooks(\'beforeAll\');\n   111→\n   112→    // 运行所有测试套件\n   113→    if (this.options.parallel) {\n   114→      await this.runSuitesParallel();\n   115→    } else {\n   116→      await this.runSuitesSequential();\n   117→    }\n   118→\n   119→    // 运行结束钩子\n   120→    await this.runGlobalHooks(\'afterAll\');\n   121→\n   122→    this.results.duration = Date.now() - startTime;\n   123→\n   124→    // 生成报告\n   125→    this.reporters.forEach(reporter => {\n   126→      if (reporter.onEnd) reporter.onEnd(this.results);\n   127→    });\n   128→\n   129→    return this.results;\n   130→  }\n   131→\n   132→  // 顺序运行测试套件\n   133→  async runSuitesSequential() {\n   134→    for (const [suiteName, suite] of this.suites) {\n   135→      await this.runSuite(suite);\n   136→    }\n   137→  }\n   138→\n   139→  // 并行运行测试套件\n   140→  async runSuitesParallel() {\n   141→    const promises = [];\n   142→    for (const [suiteName, suite] of this.suites) {\n   143→      promises.push(this.runSuite(suite));\n   144→    }\n   145→    await Promise.all(promises);\n   146→  }\n   147→\n   148→